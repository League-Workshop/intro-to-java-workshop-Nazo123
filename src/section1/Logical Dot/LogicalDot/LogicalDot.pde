void setup(){
  size(800,800);
  background(250,200,90);
}
void draw(){
 fill(#FF0015);
  ellipse(200,400,300,300);
  ellipse(600,400,300,300);
  if(mousePressed){
  fill(255,0,250);
  background(255,255,255);
  }else{
    fill(#FF0015);
    background(255,200,90);
}
ellipse(200,400,300,300);
ellipse(600,400,300,300);
}